{
  "name": "gisida-react",
  "version": "1.0.0",
  "description": "React Components Library for Gisida",
  "homepage": "https://github.com/onaio/gisida-react#readme",
  "bugs": {
    "url": "https://github.com/onaio/gisida-react/issues"
  },
  "license": "MIT",
  "author": "Ona Systems Inc.",
  "main": "build/gisida_react.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onaio/gisida-react.git"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "develop": "node scripts/develop.js",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "retainLines": true
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "mapboxgl": true,
      "$": true
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/setupTests.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)",
      "<rootDir>/test/**/?(*.)(spec|test).js?(x)"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "dependencies": {
    "ansi-regex": "^3.0.0",
    "camelcase": "^4.1.0",
    "colorbrewer": "^1.1.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gisida": "^0.0.8",
    "highcharts": "^6.0.6",
    "html-react-parser": "^0.4.2",
    "rc-slider": "^8.6.1",
    "react-redux": "^5.0.6"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.0.2",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-to-json": "^3.3.1",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "^0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "^22.4.2",
    "loader-utils": "^1.1.0",
    "node-sass": "^4.5.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "react": "^16.2",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-error-overlay": "1.0.10",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "^0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "peerDependencies": {
    "react": ">=0.16",
    "react-dom": ">=0.16"
  }
}
